            "auto_route": true, 
            "strict_route": true, 
            "type": "tun"
        }, 
        {
            "listen": "127.0.0.1", 
            "listen_port": 2333, 
            "tag": "mixed-in", 
            "type": "mixed", 
            "users": [ ]
        }
