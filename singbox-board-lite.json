            {
                "protocol": "dns", 
                "action": "hijack-dns"
            }, 
            {
                "clash_mode": "Direct", 
                "outbound": "direct"
            }, 
            {
                "clash_mode": "Proxy", 
                "outbound": "节点选择"
            }, 
            {
                "rule_set": "global", 
                "outbound": "节点选择"
            }, 
            {
                "rule_set": [
                    "direct", 
                    "geosite-cn"
                ], 
                "outbound": "direct"
