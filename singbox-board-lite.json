                "clash_mode": "Proxy", 
                "server": "remote"
            }, 
            {
                "clash_mode": "Direct", 
                "server": "local"
            }, 
            {
                "rule_set": "global", 
                "server": "remote"
            }, 
            {
                "rule_set": [
                    "direct", 
                    "geosite-cn"
                ], 
                "server": "local"
            }
        ], 
        "servers": [
            {
                "type": "https", 
                "server": "8.8.4.4", 
                "detour": "节点选择", 
                "tag": "remote"
            }, 
            {
                "type": "https", 
                "server": "223.5.5.5", 
                "tag": "local"
            }, 
            {
                "type": "hosts", 
                "path": [ ], 
                "predefined": { }, 
                "tag": "block"
            }, 
            {
                "type": "local"
            }
        ], 
        "strategy": "prefer_ipv4"
    }, 
    "experimental": {
        "cache_file": {
            "enabled": true
        }
    }, 
    "inbounds": [
        {
            "address": [
                "172.18.0.1/30", 
                "fdfe:dcba:9876::1/126"
            ], 
            "route_address": [
                "0.0.0.0/1", 
                "128.0.0.0/1", 
                "::/1", 
                "8000::/1"
